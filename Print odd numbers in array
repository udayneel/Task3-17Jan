 //Anonymous 
  let arr = [1,2,3,4,5,6,7,8,9];

 var odd = function(arr){

var arr1 =[] ;

  for (var i = 0;i<arr.length;i++){
      if(arr[i]%2 !== 0){
         arr1.push(arr[i]);
     }
  }
     return arr1;
 }
  var result = odd(arr);
  console.log(result);
 
 
